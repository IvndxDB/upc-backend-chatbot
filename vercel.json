{
  "version": 2,
  "builds": [
    {
      "src": "api/check_price.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/check_price",
      "dest": "/api/check_price.py"
    }
  ],
  "env": {
    "GEMINI_KEY": "@gemini_key",
    "OXYLABS_USERNAME": "@oxylabs_username",
    "OXYLABS_PASSWORD": "@oxylabs_password"
  }
}
